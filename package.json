{
    "name": "webpack4-lib",
    "version": "0.0.12",
    "description": "webpack4 library practice",
    "main": "dist/webpack4-lib.cjs.js",
    "browser": "dist/webpack4-lib.umd.js",
    "scripts": {
        "build": "npm run build:umd && npm run build:umd:help && npm run build:cjs",
        "build:umd": "webpack --config ./script/webpack.config.umd.js",
        "build:umd:help": "webpack --config ./script/webpack.config.umd.help.js",
        "build:cjs": "webpack --config ./script/webpack.config.cjs.js",
        "prepublishOnly": "npm run build"
    },
    "author": "xhavit",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "lodash": "^4.17.15",
        "mockjs": "^1.1.0",
        "terser-webpack-plugin": "^2.3.5",
        "uglify-js": "^3.8.1",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {},
    "peerDependencies": {
        "lodash": "^4.17.15",
        "mockjs": "^1.1.0"
    }
